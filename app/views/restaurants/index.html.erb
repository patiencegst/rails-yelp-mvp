<h1> Restaurants list  </h1>

<ul>
  <% @restaurants.each do |restaurant| %>
  <li>  <%= link_to restaurant.name, restaurant_path(restaurant) %>
    <%= link_to "Add a review", new_restaurant_review_path(restaurant) %></li>
  <% end %>
  <p> <%= link_to 'Add a restaurant', new_restaurant_path %> </p>


</ul>
